{
    "name": "coufal/laravel-http-cronjob",
    "description": "A Laravel package to trigger the scheduler via HTTP endpoint.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Coufal\\LaravelHttpCronjob\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dennis Coufal",
            "email": "dennis.coufal@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/routing": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coufal\\LaravelHttpCronjob\\Providers\\HttpCronjobServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
